	<parameter name="COMPANY" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["PAPER IMPORTS, S.A. DE C.V."]]></defaultValueExpression>
	</parameter>
	<parameter name="PROVEEDOR" isForPrompting="true" class="java.lang.String"/>
	<parameter name="FECHA" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT P.ID,X.ID,X.NOMBRE,X.PLAZO,P.MONEDA,P.DOCUMENTO,P.FECHA,P.VENCIMIENTO
,(SELECT MAX(E.fecha_embarque) FROM embarque_det D JOIN embarque E ON(E.ID=D.embarque_id) WHERE (D.factura_id=P.ID)) AS FECHA_BL
,date_add((SELECT MAX(E.fecha_embarque) FROM embarque_det D JOIN embarque E ON(E.ID=D.embarque_id) WHERE (D.factura_id=P.ID)), interval x.plazo day) as vtoyyy
,DATE(case   when DAYOFWEEK(P.vencimiento)=4 then P.vencimiento
        when DAYOFWEEK(P.vencimiento)<4 then DATE_ADD(P.vencimiento, INTERVAL -(3+DAYOFWEEK(P.vencimiento)) DAY) 
        when DAYOFWEEK(P.vencimiento)>4 then DATE_ADD(P.vencimiento, INTERVAL -(DAYOFWEEK(P.vencimiento)-4) DAY) end) as FECHA_A_PAG        
,P.TOTAL
,IFNULL((SELECT SUM(B.TOTAL) FROM aplicacion B WHERE B.FACTURA_ID=P.ID AND B.fecha<=$P{FECHA}),0) AS APLIC
,P.TOTAL-IFNULL((SELECT SUM(B.TOTAL) FROM aplicacion B WHERE B.FACTURA_ID=P.ID AND B.fecha<=$P{FECHA}),0) AS SALDO
,CASE WHEN (TO_DAYS($P{FECHA})-TO_DAYS(P.VENCIMIENTO))<=0 THEN 0 ELSE (TO_DAYS($P{FECHA})-TO_DAYS(P.VENCIMIENTO)) END AS ATR
 FROM cuenta_por_pagar P
JOIN proveedor X ON(X.ID=P.proveedor_id)
WHERE P.FECHA>'2015/07/31' AND  P.proveedor_id = $P{PROVEEDOR}
AND P.MONEDA='USD' AND P.CLASS='com.luxsoft.impapx.FacturaDeImportacion' AND
P.TOTAL-IFNULL((SELECT SUM(B.TOTAL) FROM aplicacion B WHERE B.FACTURA_ID=P.ID AND B.fecha<=$P{FECHA}),0)>1
AND (case when (SELECT MAX(E.fecha_embarque) FROM embarque_det D JOIN embarque E ON(E.ID=D.embarque_id) WHERE (D.factura_id=P.ID)) is null then P.FECHA else 
	(SELECT MAX(E.fecha_embarque) FROM embarque_det D JOIN embarque E ON(E.ID=D.embarque_id) WHERE (D.factura_id=P.ID)) end) <= $P{FECHA}
ORDER BY P.vencimiento ASC]]></queryString>

